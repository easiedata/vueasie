{
  "name": "vueasie",
  "version": "1.0.8",
  "main": "dist/easiedata.umd.js",
  "module": "dist/easiedata.esm.js",
  "unpkg": "dist/easiedata.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/easiedata.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/easiedata.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/easiedata.min.js",
    "release": "sh build/release.sh"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "bootstrap": "^3.4.1",
    "core-js": "^3.6.5",
    "inputmask": "^5.0.5",
    "v-tooltip": "^2.0.3",
    "vue": "^2.6.11",
    "vue-click-outside": "^1.1.0",
    "vue-color": "^2.8.1",
    "vue-select": "^3.11.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.2.0",
    "node-sass": "^5.0.0",
    "rollup": "^1.17.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-vue": "^5.0.1",
    "sass": "^1.32.6",
    "sass-loader": "^10.1.1",
    "vue": "^2.6.10",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.46.0",
    "webpack-dev-server": "^3.11.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off",
      "no-undef": "off",
      "no-console": "off",
      "no-unused-labels": "off",
      "no-useless-escape": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
