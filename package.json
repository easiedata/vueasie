{
  "name": "vueasie",
  "version": "1.0.2",
  "main": "dist/easiedata.umd.js",
  "module": "dist/easiedata.esm.js",
  "unpkg": "dist/easiedata.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/easiedata.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/easiedata.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/easiedata.min.js",
    "release": "sh build/release.sh"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "bootstrap": "^3.4.1",
    "core-js": "^3.6.5",
    "echarts": "^5.0.2",
    "inputmask": "^5.0.5",
    "node-sass": "^5.0.0",
    "v-tooltip": "^2.0.3",
    "vue": "^2.6.11",
    "vue-click-outside": "^1.1.0",
    "vue-color": "^2.8.1",
    "vue-select": "^3.11.2",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@pnotify/core": "^5.2.0",
    "@pnotify/mobile": "^5.2.0",
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "babel-eslint": "^10.0.3",
    "css-loader": "^5.1.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "pnotify": "^5.2.0",
    "rollup": "^1.17.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-vue": "^5.0.1",
    "sass": "^1.32.4",
    "sass-loader": "^9.0.3",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-unused-vars": "off",
      "no-undef": "off",
      "no-console": "off",
      "no-unused-labels": "off",
      "no-useless-escape": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
